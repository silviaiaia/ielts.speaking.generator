{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*).js", "dest": "/$1.js", "headers": { "Content-Type": "application/javascript" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
